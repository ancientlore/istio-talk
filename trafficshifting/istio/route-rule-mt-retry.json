{
    "type": "routerules",
    "name": "topdogmt-retry",
    "locations": [
        {
            "name": "awsqa"
        }
    ],
	"user": "${KUBE_USER}",
    "token": "${KUBE_TOKEN}",
    "namespace": "${KUBE_NAMESPACE}",
    "k8s": {
        "spec": {
            "destination": {
                "name": "topdogmt"
            }, 
            "precedence": 2,
            "route": [
                {
                    "labels": {
                        "version": "v1"
                    }
                }
            ],
            "httpReqRetries": {
                "simpleRetry": {
                    "attempts": 3,
                    "perTryTimeout": "100ms"
                }
            }
        }
    }
}
