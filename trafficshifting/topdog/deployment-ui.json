{
	"containers": [
		{
			"name": "topdog",
			"image": "ancientlore/topdog:v0.1",
			"port": 5000,
			"mincpu": "50m",
			"maxcpu": "100m",
			"minmem": "20Mi",
			"maxmem": "50Mi",
			"env": [
				{ "name": "MIDTIER", "value": "http://topdogmt:5000" },
				{ "name": "BACKEND", "value": "http://topdogbe:5000" }
			]
		}
	],
	"name": "topdogui",
	"targetGroup": "v1",
	"version": "v1",
	"targetPort": 5000,
	"locations": [
		{
			"name": "awsqa"
		},
		{
			"name": "minikube"
		}
	],
    "clusters": 1,
    "replicas": 1,
	"user": "${KUBE_USER}",
	"token": "${KUBE_TOKEN}",
	"namespace": "${KUBE_NAMESPACE}",
	"loadBalanced": true,
	"autodeploy": true,
	"deploymentStrategy": "fast",
	"healthCheck": "tcp",
	"owner": "${KUBE_OWNER}",
	"istio": true,
	"type": "deployments"
}
