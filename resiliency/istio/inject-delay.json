{
    "type": "routerules",
    "name": "webnull-test-delay",
    "locations": [
        {
            "name": "awsqa"
        }
    ],
	"user": "${KUBE_USER}",
    "token": "${KUBE_TOKEN}",
    "namespace": "${KUBE_NAMESPACE}",
    "k8s": {
        "spec": {
            "destination": {
                "name": "webnull"
            }, 
            "precedence": 3,
            "route": [
                {
                    "labels": {
                        "service": "webnull"
                    }
                }
            ],
            "match": {
                "request": {
                    "headers": {
                        "method": {
                            "exact": "GET"
                        }
                    }
                }
            },
            "httpFault": {
                "delay": {
                    "fixedDelay": "1s",
                    "percent": 5
                }
            }
        }
    }
}
